@startuml
'https://plantuml.com/sequence-diagram

    enum Candidate
    class Election {
        -{static} Election instance
        -int trump, biden
        -Election()
        +{static} Election getIntStance()
        +void vote (Candidate c)
    }
    class User{
        -String id
        +void vote (Candidate c)
    }

    User ..> Election

@enduml